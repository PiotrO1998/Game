<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0" style="-fx-background-image: url(&quot;https://i.imgur.com/TZdK4Mo.png&quot;); -fx-background-size: cover;" xmlns="http://javafx.com/javafx/8.0.231" xmlns:fx="http://javafx.com/fxml/1" fx:controller="menu.Controller">
  <children>
    <Button mnemonicParsing="false" onAction="#playGameButton" prefHeight="45.0" prefWidth="177.0" text="PLAY GAME" textFill="#150576">
      <font>
        <Font name="System Bold" size="21.0" fx:id="x1" />
      </font>
    </Button>
    <Button font="$x1" mnemonicParsing="false" onAction="#leaderBoard" prefWidth="183.0" text="LEADERBOARD" textFill="#150576" />
    <Button font="$x1" mnemonicParsing="false" onAction="#helpInstructions" prefHeight="45.0" prefWidth="177.0" text="HELP" textFill="#150576" />
    <Button font="$x1" mnemonicParsing="false" onAction="#quitButton" prefHeight="45.0" prefWidth="177.0" text="QUIT" textFill="#150576" />
    <Label fx:id="MOTD" alignment="CENTER" disable="false" prefHeight="60.0" prefWidth="479.0" text="" textFill="#150576" translateX="8.0" translateY="63.0">
      <font>
        <Font name="Arial" size="14.0" />
      </font>
    </Label>
  </children>
  <padding>
    <Insets bottom="30.0" left="10.0" right="10.0" top="30.0" />
  </padding>
</VBox>
